//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

void main ()
{
	int edad;
	cout<<"Ingrese su edad: "<<endl;
	cin>>edad;
	if (edad>=18)
		cout<<"Puede votar"<<endl;
	if (edad<18)
		cout<<"No puede votar"<<endl;
	getch();

}
